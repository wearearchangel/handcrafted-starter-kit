{
  "name": "handcrafted-starter-kit",
  "version": "1.2.3",
  "private": true,
  "dependencies": {
    "@wearearchangel/handcrafted": "^1.0.0-beta.2"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^13",
    "css-loader": "^7",
    "css-minimizer-webpack-plugin": "^7",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-webpack": "^0.13.8",
    "eslint-plugin-import": "^2.29.1",
    "html-webpack-plugin": "^5",
    "mini-css-extract-plugin": "^2",
    "rimraf": "^6",
    "sass": "^1",
